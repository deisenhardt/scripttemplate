  printf "\n"
  printf "\n"
  printf "\n\n"
  exit 1
}

sudo_check() {
  if [ "$(id -u)" -ne 0 ]; then
    printf "\n\t$0 must be run as sudo\n\n"
    usage && exit 3
  else
    sudook=1
  fi
}

invalid_input()
{
  printf "\nInvalid input, exiting\n\n"
  usage
}

